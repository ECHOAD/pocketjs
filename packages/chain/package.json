{
  "name": "@echoad/pokt-chain",
  "version": "0.3.0",
  "author": "Adrian Estevez <adrianeea.ae@gmail.com>",
  "description": "Chain Registry info for Pocket Network",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/echoad/pocketjs"
  },
  "scripts": {
    "copy": "copyfiles -f ../../LICENSE README.md package.json dist",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc && tsc -p tsconfig.esm.json && npm run copy",
    "codegen": "ts-node scripts/codegen.ts",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "copyfiles": "^2.4.1",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@chain-registry/types": "^2.0.13",
    "@chain-registry/utils": "^2.0.13"
  }
}
