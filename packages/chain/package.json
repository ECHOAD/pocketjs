{
  "name": "@echoad/pokt-chain",
  "version": "0.0.1",
  "description": "Chain Registry info for Pocket Network",
  "author": "Adrian Estevez <adrianeea.ae@gmail.com>",
  "license": "MIT",
  "main": "main/index.js",
  "typings": "types/index.d.ts",
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "files": [
    "types",
    "main"
  ],
  "scripts": {
    "build": "cross-env BABEL_ENV=production babel src --out-dir main --delete-dir-on-start --extensions \".tsx,.ts,.js\"",
    "build:ts": "tsc --project ./tsconfig.json",
    "buidl": "npm run build && npm run build:ts",
    "build:pkg": "babel-node ./scripts/build.js",
    "codegen": "npm run build:pkg && npm run format",
    "prepare": "npm run build",
    "lint": "eslint --ext .ts,.tsx,.js .",
    "format": "eslint src --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ECHOAD/pocketjs"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/ECHOAD/pocketjs/issues"
  },
  "devDependencies": {
    "@babel/cli": "7.26.4",
    "@babel/core": "7.26.0",
    "@babel/eslint-parser": "^7.25.9",
    "@babel/node": "^7.26.0",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-export-default-from": "7.25.9",
    "@babel/plugin-proposal-object-rest-spread": "7.20.7",
    "@babel/plugin-transform-runtime": "7.25.9",
    "@babel/preset-env": "7.26.0",
    "@babel/preset-typescript": "^7.17.12",
    "@chain-registry/utils": "^2.0.13",
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.17.0",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "8.18.1",
    "@typescript-eslint/parser": "8.18.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "29.7.0",
    "babel-watch": "^7.8.1",
    "chain-registry": "^2.0.13",
    "cross-env": "^7.0.2",
    "eslint": "9.30.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "globals": "15.13.0",
    "jest": "^29.7.0",
    "long": "^5.2.3",
    "prettier": "^3.4.2",
    "regenerator-runtime": "^0.14.1",
    "ts-jest": "^29.2.5",
    "typescript": "^4.3.5",
    "typescript-eslint": "8.35.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.26.0",
    "@chain-registry/types": "^2.0.13"
  }
}
